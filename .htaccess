<IfModule mod_security.c>
  SecFilterEngine Off
  SecFilterScanPOST Off
</IfModule>
#Kode untuk cache file selama 1 bulan
<IfModule mod_expires.c>
	ExpiresActive On
	<FilesMatch "\.(flv|ico|pdf|avi|mov|ppt|doc|mp3|wmv|wav|js|css|gif|jpg|jpeg|png|swf)$">
	ExpiresDefault A2419200
	</FilesMatch>
</IfModule>
# Enable gzip compression
<ifModule mod_gzip.c>
mod_gzip_on Yes
mod_gzip_dechunk Yes
mod_gzip_item_include file \.(html?|txt|css|js|php|pl)$
mod_gzip_item_include handler ^cgi-script$
mod_gzip_item_include mime ^text/.*
mod_gzip_item_include mime ^application/x-javascript.*
mod_gzip_item_exclude mime ^image/.*
mod_gzip_item_exclude rspheader ^Content-Encoding:.*gzip.*
</ifModule>
#ORIGINAL
php_value upload_max_filesize 12048M
php_value post_max_size 12048M
php_value max_execution_time 10000000
php_value max_connections_per_hour 10000000
php_value max_input_time 10000000
<filesMatch ".(flv|gif|jpg|jpeg|png|ico|swf|pdf)$.">
		Header set Cache-Control "max-age=2592000"
</filesMatch>
RewriteEngine on
RewriteCond $1 !^(index\.php|resources|robots\.txt|upload|cms)
RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} !-f
RewriteRule \.(gif|jpe?g|png|bmp)(.+)$ https://siapp.net/fn/file/notFound?$2 [NC,L]
RewriteRule ^(.+)/App/(.+)$ https://%{HTTP_HOST}/app/$2 [R=301,L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php/$1 [L,QSA]
RewriteCond %{HTTP_HOST} ^siapp.net [NC]
RewriteRule ^(.*)$ https://www.siapp.net/$1 [L,R=301]
RewriteRule ^(.*)/index\.html$ / [R=301,L]
RewriteRule (.*?)index\.php$ / [R=301,L]



